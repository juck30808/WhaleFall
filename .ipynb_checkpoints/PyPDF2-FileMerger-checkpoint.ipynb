    "\n",
    "pdfs = ['input/Intro.pdf', 'input/Intro1.pdf']\n",
    "merger = PdfFileMerger()\n",
    "\n",
    "for pdf in pdfs:\n",
    "    merger.append(pdf)\n",
    "\n",
    "merger.write(\"output/PdfFileMerger.pdf\")\n",
    "merger.close()"
